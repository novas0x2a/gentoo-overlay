/* Systemtap tapset to make it easier to trace Python */

/*
   Define python.function.entry/return:
*/
probe XXX_PROBE_NAME.function.entry = process("XXX_PROCESS_PATH").library("XXX_LIBRARY_PATH").mark("function__entry")
{
    filename = user_string($arg1);
    funcname = user_string($arg2);
    lineno = $arg3;
}
probe XXX_PROBE_NAME.function.return = process("XXX_PROCESS_PATH").library("XXX_LIBRARY_PATH").mark("function__return")
{
    filename = user_string($arg1);
    funcname = user_string($arg2);
    lineno = $arg3;
}
